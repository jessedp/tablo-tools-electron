    "@types/pubsub-js": "^1.8.2",
    "@types/react": "^17.0.2",
    "@types/react-bootstrap": "^0.32.30",
    "@types/react-dom": "^17.0.11",
    "@types/react-motion": "^0.0.32",
    "@types/react-paginate": "^6.2.1",
